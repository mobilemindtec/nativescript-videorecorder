{
	"name": "nativescript-videorecorder",
	"version": "1.3.4",
	"description": "NativeScript videorecording plugin",
	"main": "videorecorder.js",
	"nativescript": {
		"platforms": {
			"android": "2.4.1",
			"ios": "2.4.1"
		}
	},
	"dependencies": {
		"nativescript-permissions": "https://github.com/mobilemindtec/nativescript-permissions.git"
	},
	"scripts": {
		"build": "tsc",
		"demo.ios": "npm run preparedemo ; cd demo ; tns emulate ios",
		"demo.android": "npm run preparedemo ; cd demo ; tns run android",
		"preparedemo": "npm run build ; cd demo ; tns plugin remove nativescript-videorecorder ; tns plugin add .. ; tns install",
		"setup": "cd demo ; npm install ; cd .. ; npm run build ; cd demo ; tns plugin add .. ; cd .."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/triniwiz/nativescript-videorecorder.git"
	},
	"keywords": [
		"NativeScript",
		"JavaScript",
		"Android",
		"IOS",
		"video capture",
		"video recorder"
	],
	"contributors": [
		{
			"name": "Victor Sosa",
			"url": "https://github.com/victorsosa"
		}
	],
	"author": {
		"name": "Osei Fortune",
		"email": "fortune.osei@yahoo.com"
	},
	"bugs": {
		"url": "https://github.com/triniwiz/nativescript-videorecorder/issues"
	},
	"license": "MIT",
	"homepage": "https://github.com/triniwiz/nativescript-videorecorder",
	"readmeFilename": "README.md",
	"devDependencies": {
		"typescript": "^1.8.10"
	}
}
